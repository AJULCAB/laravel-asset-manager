{
  "name": "sudomabider/laravel-asset-manager",
  "description": "Allows for assets like js and css to be easily managed and called in Laravel",
  "version": "0.1.4",
  "keywords": ["Laravel", "assets", "css", "js"],
  "license": "MIT",
  "authors": [
    {
      "name": "Veo Chen",
      "email": "sudomabider@sudo.team"
    }
  ],
  "require": {
    "php": ">=5.4",
    "illuminate/view": "5.*"
  },
  "require-dev": {
    "fzaninotto/faker": "^1.6",
    "orchestra/testbench": "3.3.3|~3.4.0",
    "phpunit/phpunit": "^5.2"
  },
  "autoload": {
    "psr-4": {
      "Sudomabider\\AssetManager\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Sudomabider\\AssetManager\\Tests\\": "tests"
    }
  }
}
